set term !! ;

create procedure SP_{%object_name%}
returns ( {%columns:{%separator:,%}
    {%object_name%} {%column_datatype%}%}
)
as
begin
    for select {%columns:{%separator:, %}a.{%object_name%}%}
        from {%object_name%} a
        into {%columns:{%separator:, %}{%object_name%}%}
    do
    begin
        suspend;
    end
end!!

set term ; !!
